{
  "name": "ai-todo-list-apps",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "pnpm@10.4.1",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "npx expo export -p web",
    "eas-build-post-install": "cd .. && cd .. && export SUPABASE_URL=$EXPO_PUBLIC_SUPABASE_URL && export SUPABASE_ANON_KEY=$EXPO_PUBLIC_SUPABASE_ANON_KEY",
    "dev": "expo start --dev-client",
    "clean": "npx expo prebuild --clean && rm -rf node_modules && pnpm install",
    "lint": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "prebuild": "expo prebuild",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:preview": "eas build --platform android --profile preview"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "@supabase/supabase-js": "^2.89.0",
    "expo": "~54.0.30",
    "expo-build-properties": "^1.0.10",
    "expo-constants": "~18.0.12",
    "expo-font": "~14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.17",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.3",
    "@babel/core": "^7.20.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.1"
  },
  "private": true
}
