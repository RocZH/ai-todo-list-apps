{
  "expo": {
    "name": "AI Todo List",
    "slug": "ai-todo-list-apps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aitodolistapps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.roczh.aitodolistapps",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.roczh.aitodolistapps",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "kotlinVersion": "1.9.10",
            "compileSdk": 34,
            "targetSdk": 34,
            "minSdkVersion": 24,
            "packagingOptions": {
              "pickFirst": [
                "META-INF/**"
              ]
            },
            "buildType": "apk",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "useLegacyPackaging": false,
            "applicationId": "com.roczh.aitodolistapps"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "71def0ce-9726-493c-9548-7d4c0f00d6b9"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/71def0ce-9726-493c-9548-7d4c0f00d6b9"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
